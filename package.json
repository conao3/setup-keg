{
  "name": "setup-keg",
  "version": "0.0.1",
  "private": true,
  "description": "Github Action to set up a specific Keg version",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "release": "tsc && ncc build src/main.ts && git add -f dist/",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conao3/setup-keg.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Naoya Yamashita",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/tool-cache": "^1.5.5"
  },
  "devDependencies": {
    "@actions/io": "^1.0.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.42",
    "@types/semver": "^6.2.1",
    "@zeit/ncc": "^0.20.5",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.3"
  }
}
