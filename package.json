    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.5.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/semver": "^7.3.4",
    "@vercel/ncc": "^0.25.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
